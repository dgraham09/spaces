<div class="m-5">
  <%= form_with model: @space do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <%= f.label :price %>
    <%= f.text_field :price %>
    <%= f.submit %>
  <% end %>
  <%= link_to "Go back", dashboard_path, class: "text-decoration-none"%>
</div>
